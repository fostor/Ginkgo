@using System.Collections.Generic
@using Fostor.Ginkgo.Web
@model Fostor.Ginkgo.Web.Areas.TaskFlow.Models.Task.TaskViewModel
@{
    Layout = null;
}
<div class="panel panel-info">
    <div class="header">
        <h2>@L("ExecuteLog")</h2>
    </div>
    <div class="table-responsive datatables-container">
        <table class="table table-bordered table-condensed">
            <thead>
                <tr>
                    <th>@L("CreationTime")</th>
                    <th>@L("PhaseCode")</th>
                    <th>@L("Actor")</th>
                    <th>@L("ActorName")</th>
                    <th>@L("HandleResultName")</th>
                    <th>@L("Memo")</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var el in Model.ExecuteLogs)
                {
                    <tr>
                        <td>@el.CreationTime</td>
                        <td>@el.PhaseCode</td>
                        <td>@el.Actor</td>
                        <td>@el.ActorName</td>
                        <td>@el.HandleResultName</td>
                        <td>@el.Memo</td>
                    </tr>
                }
            </tbody>
        </table>

    </div>
</div>