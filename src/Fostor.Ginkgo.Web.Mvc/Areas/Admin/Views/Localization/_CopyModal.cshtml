@using Fostor.Ginkgo.Web.Models.Common.Modals
@using System.Collections.Generic
@using Fostor.Ginkgo
@{
    Layout = "~/Views/Shared/_Modal.cshtml";
    ViewBag.ModalTitle = L("CopyLanguageText");
}
<form name="LanguageTextCopyForm" role="form" novalidate class="form-validation form-horizontal">
    <div class="modal-body">
        <div class="form-group">
            <label class="col-md-2 control-label">@L("SourceTenancyName")<span style="color:red;">*</span></label>
            <div class="col-md-10">
                <div class="form-line">
                    <input class="form-control" type="text" name="SourceTenancyName" required>
                </div>
            </div>
        </div>
        <div class="form-group">
            <label class="col-md-2 control-label">@L("TargetTenancyName")<span style="color:red;">*</span></label>
            <div class="col-md-10">
                <div class="form-line">
                    <input class="form-control" type="text" name="TargetTenancyName" required>
                </div>
            </div>
        </div>
    </div>
</form>
@section modalfooter
    {
    <button type="button" class="btn btn-primary save-button waves-effect">@L("Save")</button>
}
<script src="~/view-resources/Admin/Localization/_CopyModal.js" asp-append-version="true"></script>









